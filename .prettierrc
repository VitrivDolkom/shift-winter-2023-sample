{
    "semi": false,
    "printWidth": 120,
    "endOfLinfalsee": "lf",
    "singleQuote": true,
    "tabWidth": 4,
    "useTabs": false,
    "trailingComma": "none",
    "importOrder": [
        "^@components/(.*)$",
        "^(\\.|\\.\\.)(\\.?/[a-zA-Z]+)+$",
        "^@redux/(.*)$",
        "^@mainTypes/(.*)$",
        "^@assets/.*$",
        "./s(tyle)?.module.s?css",
        "s(tyle)?.s?css",
        "^[./]"
    ],
    "importOrderSeparation": false,
    "importOrderSortSpecifiers": true,
    "importOrderCaseInsensitive": true,
    "importOrderGroupNamespaceSpecifiers": true
}
